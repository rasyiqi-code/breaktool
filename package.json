{
  "name": "breaktool",
  "version": "1.0.0",
  "description": "Discover. Test. Decide. The most trusted SaaS review platform, by testers you can trust.",
  "private": true,
  "author": "Breaktool Team",
  "license": "MIT",
  "homepage": "https://github.com/rasyiqi-code/breaktool#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rasyiqi-code/breaktool.git"
  },
  "bugs": {
    "url": "https://github.com/rasyiqi-code/breaktool/issues"
  },
  "keywords": [
    "saas",
    "reviews",
    "testing",
    "nextjs",
    "typescript",
    "prisma",
    "supabase",
    "stack-auth"
  ],
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "build:analyze": "ANALYZE=true next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "analyze": "node scripts/analyze-bundle.js",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "bun run prisma/seed.ts"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
